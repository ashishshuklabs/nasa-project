{
  "name": "nasa-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "start:server": "nodemon --watch server.ts --watch src/client --exec \"ts-node --esm server.ts\"",
    "start:client": "yarn build:client && vite --config vite.config.ts dev",
    "build": "tsc && vite build",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "preview": "vite preview"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "compression": "1.7.4",
    "cross-env": "^7.0.3",
    "express": "4.18.2",
    "nodemon": "^2.0.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "serialize-javascript": "^6.0.1",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "vite-plugin-svgr": "^3.2.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/concurrently": "6.4.0",
    "@types/eslint": "8.37.0",
    "@types/express": "^4.17.17",
    "@types/node": "18.15.12",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/serialize-javascript": "^5.0.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "8.0.1",
    "copyfiles": "^2.4.1",
    "styled-components": "^5.3.10",
    "typescript": "5.0.4",
    "vite": "4.3.0"
  }
}
